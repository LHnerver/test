	/* CTRL2_1_7(0x127) 控制通道选择 */
	bool ctrl_route_set_bo;			/*控制通道选择指令接收标志 */
	uint8_t ctrl_flag;				/* CTRL控制标志：0-CTRL1 1-CTRL2 2-None */
	uint8_t live_counter_ctrl2_1_7;	/* CTRL2_1_7生命信号 */
